def calculate_square_and_cube(num):
    # Calculate square and cube of the given number
    square = num ** 2
    cube = num ** 3
    return square, cube

def main():
    # Sample Input
    num = float(input("Given Number: "))
    
    # Calculate square and cube
    square, cube = calculate_square_and_cube(num)
    
    # Print the results
    print(f"Square Number: {square:.3f}")
    print(f"Cube Number: {cube:.3f}")

# Run the main function
main()
