def sum_of_squares(n):
    # Calculate the sum of squares from 1 to n
    total = sum(i**2 for i in range(1, n + 1))
    return total

def main():
    # Sample Input
    N = int(input("Enter N: "))
    
    # Calculate the sum of squares
    result = sum_of_squares(N)
    
    # Print the result
    print(f"Sum = {result}")

# Run the main function
main()
